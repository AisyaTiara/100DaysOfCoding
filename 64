import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		System.out.print("nilai i =");
		int a = sc.nextInt();
		System.out.print("nilai j =");
		int b = sc.nextInt();
		System.out.print("nilai sama dengan =");
		int c = sc.nextInt();
		for (int i = 0;  i < a; i++){
		for ( int j = 0; j < b; j++){
			if (j ==c){
				break;
			}
		System.out.println("i = " + i + ",j =" + j);
		}
		}
	}
}
