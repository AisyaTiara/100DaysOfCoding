import java.util.Scanner;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        //variabel
        System.out.print("nilai a: ");
        int a= sc.nextInt();
        System.out.print("nilai b: ");
        int b = sc.nextInt();
        
        
        
        
        boolean c =(a>b||a<b);
        //logika
        if (a>b){
        System.out.println("a lebih besar dibanding b ");
        }else if (a<b){
        System.out.println("a lebih kecil dibanding b");
    }
    }
}
