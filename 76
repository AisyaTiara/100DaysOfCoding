mport java.util.Scanner;
public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner (System.in);
		System.out.println("Huruf di bawa ini akan terganti menjadi huruf Kapital");
			String aisya = sc.nextLine();
			String tiara = aisya.toUpperCase();
		System.out.println(tiara);
	}
}
