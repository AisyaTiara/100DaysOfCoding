import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
		Scanner icha = new Scanner(System.in);
		System.out.println("berapa huruf yang dimasukaan");
		String input = icha.nextLine();
		int output = input.length();
		System.out.println("jawabannya " + output);
		
	}
}
