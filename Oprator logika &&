import java.util.Scanner;
public class Main {
	public static void main(String[] args) {
		Scanner icha = new Scanner(System.in);
		System.out.print("angka :");
		int a = icha.nextInt();
		if (a<9){
			System.out.println("angka satuan");
		}else if( a >10 && a<99);
		System.out.println("angka puluhan");
	}
}
