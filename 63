package com.mycompany.mavenproject7;

public class Mavenproject7 {

    public static void main(String[] args) {
       
        for (int i = 1; i <= 10; i++) {
            if (i%2==0) {
                System.out.println("angka "+ i +" ini benilai genap ");
               continue; 
            }
            System.out.println("angka " + i + " ini bernilai ganjil  ");
}}}
