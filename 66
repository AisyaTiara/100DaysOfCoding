package com.mycompany.aisya_tiara;
import java.util.Scanner;
public class AISYA_TIARA {
    public static void main(String[] args) {
        Scanner sc = new Scanner (System.in);
        int a =sc.nextInt();
        for (int i =1; i<=a; i++ ){
            for (int s =1; s<=i; s++){
                System.out.print("*");
            }
        System.out.println( );
